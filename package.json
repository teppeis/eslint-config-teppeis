{
  "name": "eslint-config-teppeis",
  "description": "ESLint config set for me!",
  "version": "10.2.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">= 8.10"
  },
  "main": "es2018.js",
  "files": [
    "*.js",
    ".prettierrc.json",
    ".prettierignore",
    "lib"
  ],
  "scripts": {
    "lint:js": "eslint '**/*.js' '**/.*.js' '**/*.ts'",
    "lint:json": "prettier --list-different '**/*.json'",
    "lint:json:fix": "prettier --write '**/*.json'",
    "test": "run-p -l -n -c --aggregate-output lint:js lint:json unit",
    "unit": "mocha test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jsdoc": "^15.9.5",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-unicorn": "^11.0.2",
    "prettier": "~1.18.2"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "eslint": "^6.4.0"
  },
  "homepage": "https://github.com/teppeis/eslint-config-teppeis",
  "repository": "https://github.com/teppeis/eslint-config-teppeis",
  "bugs": {
    "url": "https://github.com/teppeis/eslint-config-teppeis"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier"
  ],
  "license": "MIT"
}
