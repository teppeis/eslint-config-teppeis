{
  "name": "eslint-config-teppeis",
  "description": "ESLint config set for me!",
  "version": "18.0.0-1",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">= 18.12"
  },
  "main": "es2021.js",
  "files": [
    "*.js",
    "lib"
  ],
  "scripts": {
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "mocha test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-unicorn": "^48.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.17.1",
    "eslint": "^8.46.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "eslint": "^8.46.0",
    "prettier": "^3.0.0"
  },
  "homepage": "https://github.com/teppeis/eslint-config-teppeis",
  "repository": "https://github.com/teppeis/eslint-config-teppeis",
  "bugs": {
    "url": "https://github.com/teppeis/eslint-config-teppeis"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier"
  ],
  "license": "MIT"
}
